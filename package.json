{
  "name": "sensebycbd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "lighthouse": "node scripts/lighthouse.js",
    "lighthouse:mobile": "node scripts/lighthouse.js --mobile",
    "lighthouse:all": "npm run lighthouse && npm run lighthouse:mobile",
    "db:push": "dotenv -e .env.local -- supabase db push",
    "db:diff": "dotenv -e .env.local -- supabase db diff -f",
    "db:reset": "dotenv -e .env.local -- supabase db reset",
    "db:new-migration": "dotenv -e .env.local -- supabase migration new",
    "db:create-migration": "dotenv -e .env.local -- node scripts/create-migration.js",
    "db:check-status": "dotenv -e .env.local -- node scripts/check-supabase-status.js"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.0.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@tanstack/react-table": "^8.21.2",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "canvas": "^3.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.2.23",
    "formidable": "^3.5.2",
    "lucide-react": "^0.477.0",
    "next": "14.2.23",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "postcss": "^8",
    "react": "^18",
    "react-dom": "^18",
    "schema-dts": "^1.1.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "supabase": "^2.15.8"
